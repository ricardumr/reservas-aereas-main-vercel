<%- include('../../cabecalho.ejs')%>
    


    <div class="form-box">
        
            <h1>Usuário</h1>
            <form action="" method="post">
                <label for="nome">Nome</label>
                <input type="text" id="nome" name="nome" required><br>
                
                <label for="email">Email</label>
                <input type="text" id="email" name="email" required><br>
                

                    <label for="cpf">CPF</label>
                    <input type="number" max = "99999999999" min = "10000000000" id="cpf" name="cpf" required><br>

            
                <label for="telefone">Telefone</label>
                <input type="number" id="telefone" name="telefone" required><br>
                
                <button type="submit" class="form-btn">Adicionar Usuário</button>
            <br></br>
<a href="/admin/usuario/lst" class="form-btn">Cancelar</a>
            </form>
        
    </div>
    
    
</body>
</html>
